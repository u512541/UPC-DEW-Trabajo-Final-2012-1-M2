<h1>Listing doctors</h1>

<table>
  <tr>
    <th>Nombreapellido</th>
    <th>Administrador</th>
    <th>Email</th>
    <th>Telefono</th>
    <th>Usuariosistema</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @doctors.each do |doctor| %>
  <tr>
    <td><%= doctor.NombreApellido %></td>
    <td><%= doctor.Administrador %></td>
    <td><%= doctor.Email %></td>
    <td><%= doctor.Telefono %></td>
    <td><%= doctor.UsuarioSistema %></td>
    <td><%= link_to 'Show', doctor %></td>
    <td><%= link_to 'Edit', edit_doctor_path(doctor) %></td>
    <td><%= link_to 'Destroy', doctor, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

